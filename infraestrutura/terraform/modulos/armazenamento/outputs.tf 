output "buckets" {
  description = "Nomes dos buckets criados"
  value       = google_storage_bucket.buckets[*].name
}

output "urls" {
  description = "URLs dos buckets"
  value       = google_storage_bucket.buckets[*].url
}

output "bucket_self_links" {
  description = "Self-links dos buckets criados"
  value       = google_storage_bucket.buckets[*].self_link
}
