output "bucket_names" {
  description = "Nomes dos buckets criados"
  value = {
    data   = google_storage_bucket.data_bucket.name
    models = google_storage_bucket.models_bucket.name
    temp   = google_storage_bucket.temp_bucket.name
  }
}

output "bucket_urls" {
  description = "URLs dos buckets criados"
  value = {
    data   = "gs://${google_storage_bucket.data_bucket.name}"
    models = "gs://${google_storage_bucket.models_bucket.name}" 
    temp   = "gs://${google_storage_bucket.temp_bucket.name}"
  }
}